@{
    Layout = "~/Pages/Master.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Senast inkommet</title>
</head>
<body>
    <div class="container" style="margin-top:100px">
         
            @{var myNews = new WebShop.Classes.News(); }
            @foreach (var product in myNews.NewProducts())
            {
                <p>
                    @Html.Label("Produktnamn: ")
                    @Html.Raw(product.Title)
                    <br />
                    @Html.Label("Artikelnummer: ")
                    @Html.Raw(product.Artnr)
                </p>




            }
            
        
    </div>
</body>
</html>
